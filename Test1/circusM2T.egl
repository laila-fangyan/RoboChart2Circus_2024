}%]

[%operation Z!ApplExpr parseDifferent(): String{
	return  "("+ self.rightExpr.exprList.exprs.at(0).parseExpr() + " \\<noteq> " + self.rightExpr.exprList.exprs.at(1).parseExpr() +")";
}%]

[%operation Z!ApplExpr parseAnd(): String{
